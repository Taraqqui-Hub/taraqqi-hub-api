{
    "name": "taraqqi-hub-api",
    "version": "1.0.0",
    "description": "Equalio Backend API",
    "main": "dist/server.js",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"tsx src/server/index.ts\" \"tsx src/workers/index.ts\"",
        "worker": "tsx src/workers/index.ts",
        "build": "tsc && node esbuild.config.js",
        "start": "node dist/server.js",
        "db:generate": "drizzle-kit generate",
        "db:push": "drizzle-kit push",
        "db:studio": "drizzle-kit studio",
        "db:seed:rbac": "tsx src/db/seeds/seedRbac.ts"
    },
    "keywords": [
        "taraqqi-hub",
        "api",
        "backend"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@node-rs/bcrypt": "^1.10.0",
        "bcrypt": "^6.0.0",
        "bullmq": "^5.67.1",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "drizzle-orm": "^0.40.1",
        "express": "^4.21.2",
        "express-rate-limit": "^8.2.1",
        "helmet": "^8.1.0",
        "hpp": "^0.2.3",
        "http-status-codes": "^2.3.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "nodemailer": "^7.0.13",
        "pg": "^8.13.3",
        "postgres": "^3.3.5",
        "slugify": "^1.6.6",
        "uuid": "^9.0.1",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.7",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.23",
        "@types/hpp": "^0.2.7",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/morgan": "^1.9.9",
        "@types/node": "^20.19.4",
        "@types/nodemailer": "^7.0.9",
        "@types/pg": "^8.11.11",
        "@types/uuid": "^9.0.8",
        "concurrently": "^9.2.1",
        "drizzle-kit": "^0.30.4",
        "esbuild": "^0.19.8",
        "tsx": "^4.19.3",
        "typescript": "^5.7.2"
    }
}